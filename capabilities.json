{
  "dataRoles": [
    {
      "name": "latitude",
      "kind": "Grouping",
      "displayName": "Latitude",
      "description": "Latitude for marker placement"
    },
    {
      "name": "longitude",
      "kind": "Grouping",
      "displayName": "Longitude",
      "description": "Longitude for marker placement"
    },
    {
      "name": "tooltip",
      "kind": "GroupingOrMeasure",
      "displayName": "Tooltip",
      "description": "Additional data for tooltips"
    },
    {
      "name": "adminCode",
      "kind": "GroupingOrMeasure",
      "displayName": "Admin Code",
      "description": "Administrative code number"
    },
    {
      "name": "choroplethTooltip",
      "kind": "GroupingOrMeasure",
      "displayName": "Choropleth Tooltip",
      "description": "Data to display in choropleth tooltips"
    }
  ],
  "dataViewMappings": [
    {
      "table": {
        "rows": {
          "select": [
            {
              "for": {
                "in": "latitude"
              }
            },
            {
              "for": {
                "in": "longitude"
              }
            },
            {
              "for": {
                "in": "tooltip"
              }
            },
            {
              "for": {
                "in": "adminCode"
              }
            },
            {
              "for": {
                "in": "choroplethTooltip"
              }
            }
          ]
        }
      }
    }
  ],
  "objects": {
    "mapSettings": {
      "displayName": "Map Settings",
      "properties": {
        "baseMapUrl": {
          "displayName": "Base Map GeoJSON URL",
          "description": "Public URL to load base map GeoJSON data from",
          "type": {
            "text": true
          }
        },
        "disputedBordersUrl": {
          "displayName": "Disputed Borders GeoJSON URL",
          "description": "Public URL to load disputed borders GeoJSON data from",
          "type": {
            "text": true
          }
        }
      }
    }
  },
  "privileges": [
    {
      "name": "WebAccess",
      "essential": true,
      "parameters": ["https://*.microsoft.com", "https://*"]
    }
  ]
}
